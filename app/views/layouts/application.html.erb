<!DOCTYPE html>
<html>
  <head>
    <title>Payalogue + Api Example</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>

  <body onunload="">
    <div id="container">
      <div id="table_unit">
        <% if flash[:error] %>
          <div class="alert alert-error">
            <%= flash[:error].html_safe %>
          </div>
        <% end %>
        <%= yield %>
      </div>
    </div>
  </body>
</html>